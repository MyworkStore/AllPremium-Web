{
  "expo": {
    "name": "AllPremium",
    "slug": "AllPremium",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/all-icon.PNG",
    "splash": {
      "image": "./assets/images/all-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.gosoft.AllPremium",
      "permissions": [
        "CAMERA",
        "MANAGE_DOCUMENTS"
      ]
    }
  }
}
